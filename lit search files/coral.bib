@article{ WOS:000228946500002,
Author = {Karako-Lampert, S and Katcoff, DJ and Achituv, Y and Dubinsky, Z and
   Stambler, N},
Title = {Responses of <i>Symbiodinium microadriaticum</i> clade B to different
   environmental conditions},
Journal = {JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY},
Year = {2005},
Volume = {318},
Number = {1},
Pages = {11-20},
Month = {MAY 4},
Abstract = {The effects of various environmental parameters on zooxanthellae
   isolated from the sea anemone Condylactis gigantea were studied under
   controlled conditions in the laboratory. We determined that the
   zooxanthellae, identified as Symbiodinium microadriaticum (by Trench.
   B.) belong to clade B. These algae were exposed to a range of
   temperatures (17, 21.7, 26 degrees C), light intensities (25, 30, 45, 85
   mu mol q m(-2) s(-1)) and nutrient regimes. While growth rate was
   relatively independent of treatment, respiration increased significantly
   with temperature. Both light and temperature did have a significant
   effect on photosynthetic parameters.
   The cultured zooxanthellae responded to the environment in ways similar
   to those of freshly isolated ones, and survived under a wide range of
   temperatures (17, 21.7, 26 degrees C).
   In general, clade B seems to be flexible and well adapted to the
   temperature range encountered in seawater throughout its wide global
   distribution. (c) 2004 Elsevier B.V. All rights reserved.},
