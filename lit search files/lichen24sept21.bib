@article{ WOS:000340574500015,
Author = {Lattman, Hakan and Bergman, Karl-Olof and Rapp, Malin and Talle, Malin
   and Westerberg, Lars and Milberg, Per},
Title = {{Decline in lichen biodiversity on oak trunks due to urbanization}},
Journal = {{NORDIC JOURNAL OF BOTANY}},
Year = {{2014}},
Volume = {{32}},
Number = {{4}},
Pages = {{518-528}},
Month = {{AUG}},
Abstract = {{Biodiversity often suffers from urbanization. In the present study, we
   focused on how the duration of urbanization affects the richness of 17
   epiphytic lichen species and their cover on large oaks in urban
   environments in a city of 100 000 inhabitants in southeast Sweden. We
   also surveyed trees in adjacent rural areas, selected to have similar
   distributions of tree trunk circumference and surrounding oak density
   (within 300 m). Lichen richness and cover were lower on urban trees
   compared to rural trees. Furthermore, richness and cover decreased with
   the length of time that urban trees had been surrounded by houses. Most
   of the species that were analysed demonstrated a decline in occurrence
   with respect to the duration of housing development. The reduction in
   the probability of occurrence varied from 60\% (Calicium viride, Evernia
   prunastri), 80\% (Chrysothrix candelaris) to 90\% (Ramalina spp.) during
   the considered 160-year period of urbanization. Therefore, even if
   valuable trees survive over the course of development, their lichen
   biota is likely to become depleted over time.}},
DOI = {{10.1111/j.1756-1051.2013.00413.x}},
ISSN = {{0107-055X}},
