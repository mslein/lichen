@article{ WOS:000227872100002,
Author = {Backor, M and Fahselt, D},
Title = {{Tetrazolium reduction as an indicator of environmental stress in lichens
   and isolated bionts}},
Journal = {{ENVIRONMENTAL AND EXPERIMENTAL BOTANY}},
Year = {{2005}},
Volume = {{53}},
Number = {{2}},
Pages = {{125-133}},
Month = {{APR}},
Abstract = {{A rapid and inexpensive quantitative method was developed for evaluating
   the physiological condition of intact lichens and cultured symbionts.
   Formation of the colored product triphenyl formazan (TPF), which is
   produced after reduction of triphenyltetrazolium chloride (TTC), was
   used as an indication of dehydrogenase activity. Low pH, exposure to
   copper, supplemental UV and temperature extremes significantly reduced
   the capacity of cultured lichens and bionts to produce formazan,
   suggesting each of these treatments may produce physiological damage. It
   is projected that this method may also be suitable for monitoring impact
   of various environmental stresses on field-collected lichens. (c) 2004
   Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.envexpbot.2004.03.007}},
ISSN = {{0098-8472}},
Unique-ID = {{WOS:000227872100002}},
}

@article{ WOS:000262556600013,
